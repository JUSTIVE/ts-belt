{
	"name": "@mobily/ts-belt",
	"description": "ðŸ”§ Fast, modern, and practical utility library for FP in TypeScript.",
	"version": "4.0.0-rc.5",
	"main": "./dist",
	"module": "./dist/index.mjs",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/types/index.d.ts"
		},
		"./pipe": {
			"import": "./dist/pipe.mjs",
			"require": "./dist/pipe.js",
			"types": "./dist/types/pipe.d.ts"
		},
		"./flow": {
			"import": "./dist/flow.mjs",
			"require": "./dist/flow.js",
			"types": "./dist/types/flow.d.ts"
		},
		"./Array": {
			"import": "./dist/Array.mjs",
			"require": "./dist/Array.js",
			"types": "./dist/types/Array/index.d.ts"
		},
		"./AsyncData": {
			"import": "./dist/AsyncData.mjs",
			"require": "./dist/AsyncData.js",
			"types": "./dist/types/AsyncData/index.d.ts"
		},
		"./AsyncDataResult": {
			"import": "./dist/AsyncDataResult.mjs",
			"require": "./dist/AsyncDataResult.js",
			"types": "./dist/types/AsyncDataResult/index.d.ts"
		},
		"./AsyncOption": {
			"import": "./dist/AsyncOption.mjs",
			"require": "./dist/AsyncOption.js",
			"types": "./dist/types/AsyncOption/index.d.ts"
		},
		"./AsyncResult": {
			"import": "./dist/AsyncResult.mjs",
			"require": "./dist/AsyncResult.js",
			"types": "./dist/types/AsyncResult/index.d.ts"
		},
		"./Bool": {
			"import": "./dist/Bool.mjs",
			"require": "./dist/Bool.js",
			"types": "./dist/types/Bool/index.d.ts"
		},
		"./Dict": {
			"import": "./dist/Dict.mjs",
			"require": "./dist/Dict.js",
			"types": "./dist/types/Dict/index.d.ts"
		},
		"./Function": {
			"import": "./dist/Function.mjs",
			"require": "./dist/Function.js",
			"types": "./dist/types/Function/index.d.ts"
		},
		"./Guards": {
			"import": "./dist/Guards.mjs",
			"require": "./dist/Guards.js",
			"types": "./dist/types/Guards/index.d.ts"
		},
		"./Number": {
			"import": "./dist/Number.mjs",
			"require": "./dist/Number.js",
			"types": "./dist/types/Number/index.d.ts"
		},
		"./Option": {
			"import": "./dist/Option.mjs",
			"require": "./dist/Option.js",
			"types": "./dist/types/Option/index.d.ts"
		},
		"./Result": {
			"import": "./dist/Result.mjs",
			"require": "./dist/Result.js",
			"types": "./dist/types/Result/index.d.ts"
		},
		"./String": {
			"import": "./dist/String.mjs",
			"require": "./dist/String.js",
			"types": "./dist/types/String/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"files": ["dist", "*.md", "package.json", "index.js.flow"],
	"scripts": {
		"build": "bun foy -c ./scripts/build.ts",
		"clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
		"test": "bun foy -c ./scripts/test.ts",
		"re:clean": "bun rescript clean",
		"re:build": "bun rescript build",
		"re:dev": "bun rescript build -w",
		"transform": "bun foy -c ./scripts/transform.ts",
		"generate": "bun foy -c ./scripts/generate.ts"
	},
	"engines": {
		"node": ">= 10.*"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"keywords": [
		"typescript",
		"flow",
		"functional-programming",
		"functional",
		"programming",
		"fp",
		"monad",
		"option",
		"option-type",
		"result",
		"result-type",
		"array",
		"utility",
		"utils"
	],
	"author": "Marcin Dziewulski <hello@mobily.pl>",
	"homepage": "https://github.com/mobily/ts-belt#readme",
	"bugs": "https://github.com/mobily/ts-belt/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/mobily/ts-belt.git"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.0",
		"@rollup/plugin-commonjs": "^23.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/pluginutils": "^5.1.0",
		"@ryyppy/rescript-promise": "^2.1.0",
		"@types/jest": "^29.5.13",
		"@types/jscodeshift": "^0.11.11",
		"all-contributors-cli": "^6.26.1",
		"common-tags": "^1.8.2",
		"coveralls": "^3.1.1",
		"foy": "^0.2.33",
		"gentype": "^4.5.0",
		"globby": "^11.1.0",
		"jest": "^29.7.0",
		"jscodeshift": "^0.13.1",
		"node-fetch": "^3.3.2",
		"rescript": "^10.1.4",
		"rollup": "^3.29.4",
		"rollup-plugin-terser": "^7.0.2",
		"ts-expect": "^1.3.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.2"
	},
	"cacheDirectories": ["node_modules"]
}
